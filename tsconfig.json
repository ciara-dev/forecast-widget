{
  "compilerOptions": {
    "jsx": "react-jsx", // For React 17+ with the new JSX transform
    "target": "ES6", // Or a higher version, depending on your needs
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  }
}
